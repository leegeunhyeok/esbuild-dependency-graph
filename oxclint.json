{
  "$schema": "https://raw.githubusercontent.com/oxc-project/oxc/oxlint_v0.15.2/npm/oxlint/configuration_schema.json",
  "plugins": ["import", "typescript", "oxc"],
  "env": {
    "node": true
  },
  "ignorePatterns": [],
  "categories": {
    "correctness": "warn"
  },
  "rules": {
    "eqeqeq": "off",
    "import/no-cycle": "error",
    "no-default-export": "error",
    "no-explicit-any": "error",
    "no-non-null-assertion": "error",
    "no-console": "error"
  }
}
